{
  "name": "todo-raptor",
  "version": "0.0.10",
  "type": "module",
  "repository": {
    "url": "https://github.com/jabedek/todo-raptor"
  },
  "scripts": {
    "dev": "vite --port 8000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "up-patch": "npm version patch --no-git-tag-version",
    "up-minor": "npm version minor --no-git-tag-version",
    "up-major": "npm version major --no-git-tag-version",
    "pub-gh": "npm run up-patch  &&  git add .  &&  git commit -m 'next'  &&  git push"
  },
  "dependencies": {
    "@iconify/json": "^2.2.50",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/firebase": "^3.2.1",
    "@types/react-beautiful-dnd": "^13.1.4",
    "firebase": "^9.19.1",
    "frotsi": "^1.3.96",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "vite-require": "^0.2.3"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.62.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.1.0"
  }
}
