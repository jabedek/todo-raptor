{
  "name": "todo-raptor",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "url": "https://github.com/jabedek/todo-raptor"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "up-patch": "npm version patch --no-git-tag-version",
    "up-minor": "npm version minor --no-git-tag-version",
    "up-major": "npm version major --no-git-tag-version",
    "pub-gh": "npm run up-patch  &&  git add .  &&  git commit -m 'next'  &&  git push"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-require": "^0.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
